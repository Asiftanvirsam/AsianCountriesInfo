<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sayem.sayem.countryprofile">

    <application
        android:allowBackup="true"
        android:icon="@drawable/worldwide"
        android:label="Asian Countries Info"
        android:roundIcon="@drawable/worldwide"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.sayem.sayem.countryprofile.FirstScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.sayem.sayem.countryprofile.profile" />
        <activity android:name="com.sayem.sayem.countryprofile.MainActivity" />
        <activity android:name="com.sayem.sayem.countryprofile.AboutActivity" />
        <activity android:name="com.sayem.sayem.countryprofile.Empty1"></activity>
    </application>

</manifest>